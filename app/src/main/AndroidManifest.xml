<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trisaa.android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TransitionActivity"
            android:label="TransitionActivity"></activity>
        <activity
            android:name=".ShareElementsActivity"
            android:label="ShareElementsActivity"
            android:theme="@style/AppTheme.Explode"></activity>
        <activity
            android:name=".RevealActivity"
            android:label="RevealActivity"
            ></activity>
    </application>

</manifest>